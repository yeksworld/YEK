public class Question10 {
public static void main(String[] args) {

	int hour,minute,second;
	String A,P;
	hour=11;
	minute=33;
	second=44;
	A="Am";
	P="Pm";
	System.out.println(hour+":"+minute+":"+second+" "+ A);
}
}
